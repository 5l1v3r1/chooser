{
    "name": "cc-chooser",
    "license": "MIT",
    "description": "GSoC 2019 - The new and improved CC license chooser [WIP]",
    "private": true,
    "scripts": {
        "build-css": "node-sass --omit-source-map-url src/styles/sass/bulma-mods.scss src/styles/bulma-mods.css",
        "lint": "./node_modules/.bin/eslint src/*.js",
        "build-prod": "npm run build && npm run webpack",
        "build": "npm run build-css && npm run lint",
        "webpack": "node ./build/pre-webpack.js && npx webpack --config ./build/webpack.config.js"
    },
    "devDependencies": {
        "bulma": "^0.7.5",
        "concat": "^1.0.3",
        "copy-webpack-plugin": "^5.0.3",
        "css-loader": "^3.1.0",
        "eslint": "^5.16.0",
        "eslint-plugin-vue": "^5.2.2",
        "jest": "^24.8.0",
        "mini-css-extract-plugin": "^0.8.0",
        "node-sass": "^4.12.0",
        "style-loader": "^0.23.1",
        "webpack": "^4.33.0",
        "webpack-cli": "^3.3.3"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/creativecommons/cc-chooser.git"
    },
    "dependencies": {
        "@creativebulma/bulma-collapsible": "^1.0.4",
        "bulma-switch": "^2.0.0",
        "cool-checkboxes-for-bulma.io": "^1.1.0",
        "vue": "^2.6.10"
    }
}
